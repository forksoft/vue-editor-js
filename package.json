  "keywords": [
    "editorjs",
    "vue-editor-js",
    "vue3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/forksoft/vue-editor-js.git"
  },
  "bugs": {
    "url": "https://github.com/forksoft/vue-editor-js/issues"
  },
  "version": "4.0.3",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "deps:update": "npx npm-check-updates -u && npm install"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue-editor-js.umd.js",
  "module": "./dist/vue-editor-js.es.js",
