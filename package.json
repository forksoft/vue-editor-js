    }
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.28.2",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@types/node": "^18.18.9",
    "@vitejs/plugin-vue": "^4.5.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.22"
  },
  "sideeffects": false
}
